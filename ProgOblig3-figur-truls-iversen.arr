use context essentials2021
beside(
overlay(ellipse(20, 90, "solid", "black"),
  overlay(ellipse(50, 100, "solid", "pale-green"),
    rotate(-20, overlay(ellipse(230, 105, "solid", "goldenrod"),
          ellipse(250, 125, "solid", "black"))))),

flip-horizontal(overlay(ellipse(20, 90, "solid", "black"),
  overlay(ellipse(50, 100, "solid", "pale-green"),
    rotate(-20, overlay(ellipse(230, 105, "solid", "goldenrod"),
            ellipse(250, 125, "solid", "black")))))))
